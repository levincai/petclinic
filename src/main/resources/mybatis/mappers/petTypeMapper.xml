<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tk.puncha.mappers.PetTypeMapper">
  <select id="getAllTypes" resultType="PetType">
    SELECT * FROM types
  </select>

  <select id="getTypeById" resultType="PetType">
    SELECT * FROM types where id = #{id}
  </select>
</mapper>
